{
  "name": "@density/charts",
  "version": "0.3.5",
  "description": "A bunch of charts we use in all of our dashboards",
  "main": "charts/dist/index.js",
  "scripts": {
    "start": "npm run storybook",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "installall": "for i in $(find charts/* -type d ! -path '*node_modules*' ! -name 'template' ); do cd $i && npm install && cd -; done"
  },
  "keywords": [
    "charts",
    "react",
    "graph",
    "density"
  ],
  "author": "Density <team+javascript@density.io>",
  "license": "MIT",
  "dependencies": {
    "create-react-class": "^15.5.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.24.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "css-loader": "^0.28.1",
    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "react-dom": "^15.4.2",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.17.0",
    "webpack": "^2.3.2"
  }
}
